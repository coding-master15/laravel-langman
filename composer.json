{
    "name": "coding-master15/laravel-langman",
    "description": "Manage language files with ease. Forked to support laravel 7",
    "keywords": ["laravel", "localization", "multilingual"],
    "homepage": "https://github.com/themsaid/laravel-langman",
    "license": "MIT",
    "authors": [
        {
            "name": "Smarter Vision",
            "email": "smartersvision@gmail.com"
        },
        {
            "name": "Mohamed Said",
            "email": "theMohamedSaid@gmail.com",
            "homepage": "https://themsaid.github.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.13.0",
        "illuminate/console": "^10.13.0",
        "illuminate/filesystem": "^10.13.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.1.3",
        "orchestra/testbench": "^8.5.5",
        "orchestra/database": "^6.0.0",
        "mockery/mockery": "^1.5.1"
    },
    "autoload": {
        "psr-4": {
            "Themsaid\\Langman\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themsaid\\Langman\\LangmanServiceProvider"
            ]
        }
    }
}
